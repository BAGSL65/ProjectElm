{"ast":null,"code":"import Footer from '../components/Footer.vue';\nexport default {\n  name: 'Register',\n  data() {\n    return {\n      user: {\n        userId: '',\n        password: '',\n        userName: '',\n        userSex: 1\n      },\n      confirmPassword: ''\n    };\n  },\n  methods: {\n    checkUserId() {\n      this.$axios.get('users/' + this.user.userId, this.$qs.stringify({})).then(response => {\n        if (response.data == 1) {\n          this.user.userId = '';\n          alert('此手机号码已存在！');\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    register() {\n      if (this.user.userId == '') {\n        alert('手机号码不能为空！');\n        return;\n      }\n      if (this.user.password == '') {\n        alert('密码不能为空！');\n        return;\n      }\n      if (this.user.password != this.confirmPassword) {\n        alert('两次输入的密码不一致！');\n        return;\n      }\n      if (this.user.userName == '') {\n        alert('用户名不能为空！');\n        return;\n      }\n      if (this.user.userId.length != 11) {\n        alert('联系人电话格式不符！');\n        return;\n      }\n      //注册请求\n      this.$axios.post('users/', this.user).then(response => {\n        if (response.data > 0) {\n          alert('注册成功！');\n          this.$router.go(-1);\n        } else {\n          alert('注册失败！');\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  },\n  components: {\n    Footer\n  }\n};","map":{"version":3,"names":["Footer","name","data","user","userId","password","userName","userSex","confirmPassword","methods","checkUserId","$axios","get","$qs","stringify","then","response","alert","catch","error","console","register","length","post","$router","go","components"],"sources":["D:\\Comprehensive_HomeWork\\elm-master\\elm-master\\P3\\elmclient\\src\\views\\Register.vue"],"sourcesContent":["<template>\n\t<div class=\"wrapper\">\n\t\t<!-- header部分 -->\n\t\t<header>\n\t\t\t<p>用户注册</p>\n\t\t</header>\n\t\t<!-- 表单部分 -->\n\t\t