t<img :src=\"item.food.foodImg\">\n\t\t\t\t\t<p>{{item.food.foodName}} x {{item.quantity}}</p>\n\t\t\t\t</div>\n\t\t\t\t<p>&#165;{{item.food.foodPrice*item.quantity}}</p>\n\t\t\t</li>\n\t\t</ul>\n\n\t\t<!--订单配送费-->\n\t\t<div class=\"order-deliveryfee\">\n\t\t\t<p>配送费</p>\n\t\t\t<p>&#165;{{business.deliveryPrice}}</p>\n\t\t</div>\n\t\t<!--使用积分-->\n\t\t\t\t\t<div class=\"use-credit\" id=\"u1\" @click=\"integralShow\">\n\t\t\t\t\t\t<p>使用积分请点击</p>\n\t\t\t\t\t</div>\n\t\t\t\t\t<div class=\"using-credit\" id=\"u2\" v-show=\"isShowintegral\">\n\t\t\t\t\t\t<input type=\"credit\"  v-model=\"useIntegral\" placeholder=\"使用积分数(100点积分=1元,最高5000点)\">\n\t\t\t\t\t</div>\n\t\t<!--合计-->\n\t\t<div class=\"total\">\n\t\t\t<div class=\"total-left\">&#165;{{totalPrice}}</div>\n\t\t\t<div class=\"total-right\" @click=\"toPayment\">去支付</div>\n\t\t</div>\n\t</div>\n</template>\n\n<script>\n\texport default {\n\t\tname: 'Orders',\n\t\tdata() {\n\t\t\treturn {\n\t\t\t\tbusinessId: this.$route.query.businessId,\n\t\t\t\tbusiness: {},\n\t\t\t\tuser: {},\n\t\t\t\tcartArr: [],\n\t\t\t\tdeliveryaddress: {},\n\t\t\t\tconsignee: {},\n\t\t\t\tisShowintegral:false,\n\t\t\t\tuseIntegral:''\n\t\t\t}\n\t\t},\n\t\tcreated() {\n\t\t\tthis.user = this.$getSessionStorage('user');\n\t\t\tthis.deliveryaddress = this.$getLocalStorage(this.user.userId);\n            \n\t\t\t//切换收货人的信息\n\t\t\tif (this.deliveryaddress == null) {\n\t\t\t\tthis.consignee.contactName = this.user.userName;\n\t\t\t\tthis.consignee.contactSex = this.user.userSex;\n\t\t\t\tthis.consignee.contactTel = this.user.userId;\n\t\t\t}\n\t\t\telse{\n\t\t\t\tthis.consignee.contactName = this.deliveryaddress.contactName;\n\t\t\t\tthis.consignee.contactSex = this.deliveryaddress.contactSex;\n\t\t\t\tthis.consignee.contactTel = this.deliveryaddress.contactTel;\n\t\t\t}\n\t\t\t\n\t\t\t//跟新积分信息\n\t\t\tthis.$axios.post('UserController/getIntegral', this.$qs.stringify({\n\t\t\t\tuserId: this.user.userId\n\t\t\t})).then(response => {\n\t\t\t\tthis.user.integral = response.data;\n\t\t\t}).catch(error => {\n\t\t\t\tconsole.error(error);\n\t\t\t});\n\n\t\t\t//查询当前商家\n\t\t\tthis.$axios.post('BusinessController/getBusinessById', this.$qs.stringify({\n\t\t\t\tbusinessId: this.businessId\n\t\t\t})).then(response => {\n\t\t\t\tthis.business = response.data;\n\t\t\t}).catch(error => {\n\t\t\t\tconsole.error(error);\n\t\t\t});\n\n\t\t\t//查询当前用户在购物车中的当前商家食品列表\n\t\t\tthis.$axios.post('CartController/listCart', this.$qs.stringify({\n\t\t\t\tuserId: this.user.userId,\n\t\t\t\tbusinessId: this.businessId\n\t\t\t})).then(response => {\n\t\t\t\tthis.cartArr = response.data;\n\t\t\t}).catch(error => {\n\t\t\t\tconsole.error(error);\n\t\t\t});\n\t\t\t\n\t\t},\n\t\tcomputed: {\n\t\t\ttotalPrice() {\n\t\t\t\tlet totalPrice = 0;\n\t\t\t\tfor (let cartItem of this.cartArr) {\n\t\t\t\t\ttotalPrice += cartItem.food.foodPrice * cartItem.quantity;\n\t\t\t\t}\n\t\t\t\ttotalPrice += this.business.deliveryPrice;\r\n\t\t\t\ttotalPrice -= this.useIntegral/100;\n\t\t\t\treturn totalPrice;\n\t\t\t}\n\t\t},\n\t\tfilters: {\n\t\t\tsexFilter(value) {\n\t\t\t\treturn value == 1 ? '先生' : '女士';\n\t\t\t}\n\t\t},\n\t\tmethods: {\n\t\t\tintegralShow() {\n\t\t\t\tthis.isShowintegral = !this.isShowintegral;\n\t\t\t},\n\t\t\ttoUserAddress() {\n\t\t\t\tthis.$router.push({\n\t\t\t\t\tpath: '/userAddress',\n\t\t\t\t\tquery: {\n\t\t\t\t\t\tbusinessId: this.businessId\n\t\t\t\t\t}\n\t\t\t\t});\n\t\t\t},\n\t\t\ttoPayment() {\n\t\t\t\tif (this.deliveryaddress == null) {\n\t\t\t\t\talert('请选择送货地址！');\n\t\t\t\t\treturn;\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\tif(this.useIntegral>this.user.integral)\n\t\t\t\t{ \n\t\t\t\t\talert('使用积分数值，超过已有积分');\n\t\t\t\t\tthis.useIntegral='';\n\t\t\t\t\treturn;\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\tif(this.useIntegral>5000)\n\t\t\t\t{ \n\t\t\t\t\talert('使用积分超过允许范围');\n\t\t\t\t\tthis.useIntegral='';\n\t\t\t\t\treturn;\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\tif(this.totalPrice<=0)\n\t\t\t\t{ \n\t\t\t\t\talert('不能超越订单本身价值');\n\t\t\t\t\tthis.useIntegral='';\n\t\t\t\t\treturn;\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\t//创建有积分订单\n\t\t\t\tif(this.useIntegral>0){\n\t\t\t\tthis.$axios.post('OrdersController/createIntegralOrders', this.$qs.stringify({\n\t\t\t\t\tuserId: this.user.userId,\r\n\t\t\t\t\tbusinessId: this.businessId,\r\n\t\t\t\t\tdaId: this.deliveryaddress.daId,\r\n\t\t\t\t\torderTotal: this.totalPrice,\n\t\t\t\t\tintegralcost: this.useIntegral\n\t\t\t\t})).then(response => {\n\t\t\t\t\tlet orderId = response.data;\r\n\t\t\t\t\tif (orderId > 0) {\r\n\t\t\t\t\t\tthis.$router.push({\r\n\t\t\t\t\t\t\tpath: '/payment',\r\n\t\t\t\t\t\t\tquery: {\r\n\t\t\t\t\t\t\t\torderId: orderId\r\n\t\t\t\t\t\t\t}\r\n\t\t\t\t\t\t});\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\talert('创建订单失败！');\r\n\t\t\t\t\t}\n\t\t\t\t}).catch(error => {\n\t\t\t\t\tconsole.error(error);\n\t\t\t\t});\n                }\n\t\t\t\t//创建无积分订单\r\n\t\t\t\telse{\n\t\t\t\tthis.$axios.post('OrdersController/createOrders', this.$qs.stringify({\n\t\t\t\t\tuserId: this.user.userId,\n\t\t\t\t\tbusinessId: this.businessId,\n\t\t\t\t\tdaId: this.deliveryaddress.daId,\n\t\t\t\t\torderTotal: this.totalPrice\n\t\t\t\t})).then(response => {\n\t\t\t\t\tlet orderId = response.data;\n\t\t\t\t\tif (orderId > 0) {\n\t\t\t\t\t\tthis.$router.push({\n\t\t\t\t\t\t\tpath: '/payment',\n\t\t\t\t\t\t\tquery: {\n\t\t\t\t\t\t\t\torderId: orderId\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t});\n\t\t\t\t\t} else {\n\t\t\t\t\t\talert('创建订单失败！');\n\t\t\t\t\t}\n\t\t\t\t}).catch(error => {\n\t\t\t\t\tconsole.error(error);\n\t\t\t\t});\n\t\t\t\t}\r\n\t\t\t},\n\t\t\tgetSEX(num){\n\t\t\t\tif(num == 1)\n\t\t\t\t{\n\t\t\t\t\treturn '先生'\n\t\t\t\t}\n\t\t\t\tif(num == 0)\n\t\t\t\t{\n\t\t\t\t\treturn '女士'\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t}\n</script>\n\n<style scoped>\n\t/**总容器*/\n\t.wrapper {\n\t\twidth: 100%;\n\t\theight: 100%;\n\t}\n\n\t/**头部*/\n\t.wrapper header {\n\t\twidth: 100%;\n\t\theight: 12vw;\n\t\tbackground-color: #0097FF;\n\n\t\tcolor: #fff;\n\t\tfont-size: 4.8vw;\n\n\t\tposition: fixed;\n\t\tleft: 0;\n\t\ttop: 0;\n\t\tz-index: 10