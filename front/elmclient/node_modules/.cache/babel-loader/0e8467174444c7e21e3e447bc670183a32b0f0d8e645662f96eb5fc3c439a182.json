oller/listCart', this.$qs.stringify({\n\t\t\t\tuserId: this.user.userId,\n\t\t\t\tbusinessId: this.businessId\n\t\t\t})).then(response => {\n\t\t\t\tthis.cartArr = response.data;\n\t\t\t}).catch(error => {\n\t\t\t\tconsole.error(error);\n\t\t\t});\n\t\t\t\n\t\t},\n\t\tcomputed: {\n\t\t\ttotalPrice() {\n\t\t\t\tlet totalPrice = 0;\n\t\t\t\tfor (let cartItem of this.cartArr) {\n\t\t\t\t\ttotalPrice += cartItem.food.foodPrice * cartItem.quantity;\n\t\t\t\t}\n\t\t\t\ttotalPrice += this.business.deliveryPrice;\n\t\t\t\treturn totalPrice;\n\t\t\t}\n\t\t},\n\t\tfilters: {\n\t\t\tsexFilter(value) {\n\t\t\t\treturn value == 1 ? '先生' : '女士';\n\t\t\t}\n\t\t},\n\t\tmethods: {\n\t\t\tintegralShow() {\n\t\t\t\tthis.isShowintegral = !this.isShowintegral;\n\t\t\t},\n\t\t\ttoUserAddress() {\n\t\t\t\tthis.$router.push({\n\t\t\t\t\tpath: '/userAddress',\n\t\t\t\t\tquery: {\n\t\t\t\t\t\tbusinessId: this.businessId\n\t\t\t\t\t}\n\t\t\t\t});\n\t\t\t},\n\t\t\ttoPayment() {\n\t\t\t\tif (this.deliveryaddress == null) {\n\t\t\t\t\talert('请选择送货地址！');\n\t\t\t\t\treturn;\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\tif(this.useIntegral>this.user.integral)\n\t\t\t\t{ \n\t\t\t\t\talert('使用积分数值，超过已有积分');\n\t\t\t\t\tthis.useIntegral='';\n\t\t\t\t\treturn;\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\tif(this.useIntegral>5000)\n\t\t\t\t{ \n\t\t\t\t\talert('使用积分超过允许范围');\n\t\t\t\t\tthis.useIntegral='';\n\t\t\t\t\treturn;\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\tif(this.totalPrice<=0)\n\t\t\t\t{ \n\t\t\t\t\talert('不能超越订单本身价值');\n\t\t\t\t\tthis.useIntegral='';\n\t\t\t\t\treturn;\n\t\t\t\t}\n\t\t\t\t\n\t\t\t\t//更新积分\n\t\t\t\tif(this.useIntegral>0){\n\t\t\t\tthis.$axios.post('UserController/updateIntegralById', this.$qs.stringify({\n\t\t\t\t\tuserId: this.user.userId,\n\t\t\t\t\tIntegralquantity: this.user.integral-this.useIntegral\n\t\t\t\t})).then(response => {\n\t\t\t\t\tif (response.data > 0) {\n\t\t\t\t\t\t//alert('积分使用成功！');\n\t\t\t\t\t} else {\n\t\t\t\t\t\talert('积分使用失败！');\n\t\t\t\t\t}\n\t\t\t\t}).catch(error => {\n\t\t\t\t\tconsole.error