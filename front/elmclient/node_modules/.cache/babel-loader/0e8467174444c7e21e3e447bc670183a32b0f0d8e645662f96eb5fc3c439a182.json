{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'Orders',\n  data() {\n    return {\n      businessId: this.$route.query.businessId,\n      business: {},\n      user: {},\n      cartArr: [],\n      deliveryaddress: {},\n      consignee: {},\n      isShowintegral: false,\n      useIntegral: ''\n    };\n  },\n  created() {\n    this.user = this.$getSessionStorage('user');\n    this.deliveryaddress = this.$getLocalStorage(this.user.userId);\n\n    //切换收货人的信息\n    if (this.deliveryaddress == null) {\n      this.consignee.contactName = this.user.userName;\n      this.consignee.contactSex = this.user.userSex;\n      this.consignee.contactTel = this.user.userId;\n    } else {\n      this.consignee.contactName = this.deliveryaddress.contactName;\n      this.consignee.contactSex = this.deliveryaddress.contactSex;\n      this.consignee.contactTel = this.deliveryaddress.contactTel;\n    }\n\n    //跟新积分信息\n    this.$axios.post('UserController/getIntegral', this.$qs.stringify({\n      userId: this.user.userId\n    })).then(response => {\n      this.user.integral = response.data;\n    }).catch(error => {\n      console.error(error);\n    });\n\n    //查询当前商家\n    this.$axios.post('BusinessController/getBusinessById', this.$qs.stringify({\n      businessId: this.businessId\n    })).then(response => {\n      this.business = response.data;\n    }).catch(error => {\n      console.error(error);\n    });\n\n    //查询当前用户在购物车中的当前商家食品列表\n    this.$axios.post('CartController/listCart', this.$qs.stringify({\n      userId: this.user.userId,\n      businessId: this.businessId\n    })).then(response => {\n      this.cartArr = response.data;\n    }).catch(error => {\n      console.error(error);\n    });\n  },\n  computed: {\n    totalPrice() {\n      let totalPrice = 0;\n      for (let cartItem of this.cartArr) {\n        totalPrice += cartItem.food.foodPrice * cartItem.quantity;\n      }\n      totalPrice += this.business.deliveryPrice;\n      return totalPrice;\n    }\n  },\n  filters: {\n    sexFilter(value) {\n      return value == 1 ? '先生' : '女士';\n    }\n  },\n  methods: {\n    integralShow() {\n      this.isShowintegral = !this.isShowintegral;\n    },\n    toUserAddress() {\n      this.$router.push({\n        path: '/userAddress',\n        query: {\n          businessId: this.businessId\n        }\n      });\n    },\n    toPayment() {\n      if (this.deliveryaddress == null) {\n        alert('请选择送货地址！');\n        return;\n      }\n      if (this.useIntegral > this.user.integral) {\n        alert('使用积分数值，超过已有积分');\n        this.useIntegral = '';\n        return;\n      }\n      if (this.useIntegral > 5000) {\n        alert('使用积分超过允许范围');\n        this.useIntegral = '';\n        return;\n      }\n      if (this.totalPrice <= 0) {\n        alert('不能超越订单本身价值');\n        this.useIntegral = '';\n        return;\n      }\n\n      //更新积分\n      if (this.useIntegral > 0) {\n        this.$axios.post('UserController/updateIntegralById', this.$qs.stringify({\n          userId: this.user.userId,\n          Integralquantity: this.user.integral - this.useIntegral\n        })).then(response => {\n          if (response.data > 0) {\n            //alert('积分使用成功！');\n          } else {\n            alert('积分使用失败！');\n          }\n        }).catch(error => {\n          console.error(error);\n        });\n      }\n\n      //创建订单\n      this.$axios.post('OrdersController/createOrders', this.$qs.stringify({\n        userId: this.user.userId,\n        businessId: this.businessId,\n        daId: this.deliveryaddress.daId,\n        orderTotal: this.totalPrice\n      })).then(response => {\n        let orderId = response.data;\n        if (orderId > 0) {\n          this.$router.push({\n            path: '/payment',\n            query: {\n              orderId: orderId\n            }\n          });\n        } else {\n          alert('创建订单失败！');\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n 