eLocalStorage(this.user.userId);\n          }\n          this.listDeliveryAddressByUserId();\n        } else {\n          alert('删除地址失败！');\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    getSEX(num) {\n      if (num == 1) {\n        return '先生';\n      }\n      if (num == 0) {\n        return '女士';\n      }\n    }\n  }\n};","map":{"version":3,"names":["Footer","name","data","businessId","$route","query","user","deliveryAddressArr","created","$getSessionStorage","listDeliveryAddressByUserId","components","filters","sexFilter","value","methods","$axios","post","$qs","stringify","userId","then","response","catch","error","console","setDeliveryAddress","deliveryAddress","$setLocalStorage","$router","push","path","toAddUserAddress","editUserAddress","daId","removeUserAddress","confirm","$getLocalStorage","$removeLocalStorage","alert","getSEX","num"],"sources":["D:\\Comprehensive_HomeWork\\elm-master\\elm-master\\P3\\elmclient\\src\\views\\UserAddress.vue"],"sourcesContent":["<template>\n\t<div class=\"wrapper\">\n\t\t<!-- header部分 -->\n\t\t<header>\n\t\t\t<p>地址管理</p>\n\t\t</header>\n\t\t<!-- 地址列表部分 -->\n\t\t<ul class=\"addresslist\">\n\t\t\t<li v-for=\"item in deliveryAddressArr\">\n\t\t\t\t<div class=\"addresslist-left\" @click=\"setDeliveryAddress(item)\">\n\t\t\t\t\t<h3>{{item.contactName}}{{getSEX(item.contactSex)}} {{item.contactTel}}\n\t\t\t\t\t</h3>\n\t\t\t\t\t<p>{{item.address}}</p>\n\t\t\t\t</div>\n\t\t\t\t<div class=\"addresslist-right\">\n\t\t\t\t\t<i class=\"fa fa-edit\" @click=\"editUserAddress(item.daId)\"></i>\n\t\t\t\t\t<i class=\"fa fa-remove\" @click=\"removeUserAddress(item.daId)\"></i>\n\t\t\t\t</div>\n\t\t\t</li>\n\t\t</ul>\n\t\t<!-- 新增地址部分 -->\n\t\t<div class=\"addbtn\" @click=\"toAddUserAddress\">\n\t\t\t<i class=\"fa fa-plus-circle\"></i>\n\t\t\t<p>新增收货地址</p>\n\t\t</div>\n\t\t<!-- 底部菜单部分 -->\n\t\t<Footer></Footer>\n\t</div>\n</template>\n<script>\n\timport Footer from '../components/Footer.vue';\n\n\te