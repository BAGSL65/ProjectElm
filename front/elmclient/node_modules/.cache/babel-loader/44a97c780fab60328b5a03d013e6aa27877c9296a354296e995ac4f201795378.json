.businessId) {\n\t\t\t\t\t\t\t\tbusinessItem.quantity += cartItem.quantity;\n\t\t\t\t\t\t\t}\n\t\t\t\t\t\t}\n\t\t\t\t\t}\n\t\t\t\t\tthis.businessArr.sort();\n\t\t\t\t}).catch(error => {\n\t\t\t\t\tconsole.error(error);\n\t\t\t\t});\n\t\t\t},\n\t\t\ttoBusinessInfo(businessId) {\n\t\t\t\tthis.$router.push({path: '/businessInfo',query: {businessId: businessId}});\n\t\t\t}\n\t\t}\n\t}\n</script>\n\n<style scoped>\n\t/**总容器*/\n\t.wrapper {\n\t\twidth: 100%;\n\t\theight: 100%;\n\t}\n\n\t/**头部*/\n\t.wrapper header {\n\t\twidth: 100%;\n\t\theight: 12vw;\n\t\tbackground-color: #0097FF;\n\n\t\tcolor: #fff;\n\t\tfont-size: 4.8vw;\n\n\t\tposition: fixed;\n\t\tleft: 0;\n\t\ttop: 0;\n\t\tz-index: 1000;\n\t\t/*提高头部层次，覆盖同样离开文档流的红色图标和图片*/\n\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tjustify-content: center;\n\n\t}\n\n\t/*商家列表*/\n\t.wrapper .business {\n\t\twidth: 100%;\n\t\tmargin-top: 12vw;\n\t\tpadding-bottom: 14vw;\n\t}\n\n\t.wrapper .business li {\n\t\twidth: 100%;\n\t\tbox-sizing: border-box;\n\t\tpadding: 2.5vw;\n\t\tborder-bottom: solid 1px #ddd;\n\t\tuser-select: none;\n\t\tcursor: pointer;\n\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t}\n\n\t.wrapper .business li .business-img {\n\t\tposition: relative;\n\t}\n\n\t.wrapper .business li .business-img img {\n\t\twidth: 20vw;\n\t\theight: 20vw;\n\t}\n\n\t.wrapper .business li .business-img .img-num {\n\t\twidth: 5vw;\n\t\theight: 5vw;\n\t\tbackground-color: red;\n\t\tborder-radius: 2.5vw;\n\n\t\tcolor: #fff;\n\t\tfont-size: 3.6vw;\n\n\t\tdisplay: flex;\n\t\talign-items: center;\n\t\tjustify-content: center;\n\n\t\tposition: absolute;\n\t\tright: -1.5vw;\n\t\ttop: -1.5vw;\n\n\t}\n\n\t.wrapper .business li .info {\n\t\tmargin-left: 3vw;\n\t}\n\n\t.wrapper .business li .info h3 {\n\t\tfont-size: 3.8vw;\n\t\tcolor: #555;\n\t}\n\n\t.wrapper .business li .info p {\n\t\tfont-size: 3vw;\n\t\tcolor: #888;\n\t\tmargin-top: 2vw;\n\t}\n</style>\n"],"mappings":";AA2BC,OAAOA,MAAK,MAAO,0BAA0B;AAE7C,eAAe;EACdC,IAAI,EAAE,cAAc;EACpB